<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netstandard2.0</TargetFramework>
        <Version>0.3.2</Version>
        <LangVersion>7.3</LangVersion>
        <RepositoryType>git</RepositoryType>
        <RepositoryUrl>https://github.com/teoadal/veloimplementations</RepositoryUrl>
        <PublishRepositoryUrl>true</PublishRepositoryUrl>
        <PackageTags>mapper, json, serializer, deserializer, dependencyinjection, ioc, di</PackageTags>
        <Title>Velo</Title>
        <Description>Simple implementations of IoC-Container, Mapper and JSON Serializer/Deserializer</Description>
        <Authors>teoadal</Authors>
        <Company>teoadal</Company>
        <Product>teoadal</Product>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
    </PropertyGroup>

</Project>
