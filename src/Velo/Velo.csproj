<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netstandard2.0</TargetFramework>
        <Version>0.3.3</Version>
        <LangVersion>8</LangVersion>
        <RepositoryType>git</RepositoryType>
        <RepositoryUrl>https://github.com/teoadal/veloimplementations</RepositoryUrl>
        <PublishRepositoryUrl>true</PublishRepositoryUrl>
        <PackageTags>mapper, mediator, cqrs, json, serializer, deserializer, dependencyinjection, ioc, di</PackageTags>
        <Title>Velo</Title>
        <Description>Simple implementations of IoC-Container, Mediator (or CQRS), Mapper and JSON Serializer/Deserializer</Description>
        <Authors>teoadal</Authors>
        <Company>teoadal</Company>
        <Product>teoadal</Product>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <Nullable>disable</Nullable>
    </PropertyGroup>

    <ItemGroup>
        <AssemblyAttribute Include="System.Runtime.CompilerServices.InternalsVisibleTo">
            <_Parameter1>Velo.Benchmark</_Parameter1>
        </AssemblyAttribute>
        <AssemblyAttribute Include="System.Runtime.CompilerServices.InternalsVisibleTo">
            <_Parameter1>Velo.Tests</_Parameter1>
        </AssemblyAttribute>
    </ItemGroup>

    <ItemGroup>
      <Compile Update="ECS\SignBuilder.cs">
        <DependentUpon>Sign.cs</DependentUpon>
      </Compile>
      <Compile Update="Collections\LocalVector.Enumerator.cs">
        <DependentUpon>LocalVector.cs</DependentUpon>
      </Compile>
      <Compile Update="Collections\LocalVector.WhereEnumerator.cs">
        <DependentUpon>LocalVector.cs</DependentUpon>
      </Compile>
      <Compile Update="Collections\LocalVector.SelectEnumerator.cs">
        <DependentUpon>LocalVector.cs</DependentUpon>
      </Compile>
      <Compile Update="Collections\LocalVector.JoinEnumerator.cs">
        <DependentUpon>LocalVector.cs</DependentUpon>
      </Compile>
      <Compile Update="DependencyInjection\Factories\ArrayFactory.ArrayResolver.cs">
        <DependentUpon>ArrayFactory.cs</DependentUpon>
      </Compile>
      <Compile Update="DependencyInjection\Factories\ArrayFactory.EmptyArrayResolver.cs">
        <DependentUpon>ArrayFactory.cs</DependentUpon>
      </Compile>
      <Compile Update="Collections\LocalVector.GroupEnumerator.cs">
        <DependentUpon>LocalVector.cs</DependentUpon>
      </Compile>
      <Compile Update="Collections\LocalVector.DebugVisualizer.cs">
        <DependentUpon>LocalVector.cs</DependentUpon>
      </Compile>
    </ItemGroup>

    <ItemGroup>
      <Folder Include="Extensions" />
    </ItemGroup>
</Project>
